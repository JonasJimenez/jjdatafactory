{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Nombre de instancia de Data Factory",
			"defaultValue": "adfpruebaCSSFD"
		},
		"AzureBlobStorageCsvTwx_connectionString": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"connectionString\"de \"AzureBlobStorageCsvTwx\""
		},
		"SqlServerLocalJJJ_connectionString": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"connectionString\"de \"SqlServerLocalJJJ\""
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/CsvTwx')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "AzureBlobStorageCsvTwx",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "DelimitedText",
				"typeProperties": {
					"location": {
						"type": "AzureBlobStorageLocation",
						"container": "csvtwx"
					},
					"columnDelimiter": ",",
					"escapeChar": "\\",
					"firstRowAsHeader": true,
					"quoteChar": "\""
				},
				"schema": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/AzureBlobStorageCsvTwx')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/SqlServerSHAYA')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "SqlServerLocalJJJ",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "SqlServerTable",
				"schema": [
					{
						"name": "WELL",
						"type": "nvarchar"
					},
					{
						"name": "WELL_ACTIVE",
						"type": "nvarchar"
					},
					{
						"name": "COMPLETION",
						"type": "nvarchar"
					},
					{
						"name": "COMPLETION_ACTIVE",
						"type": "nvarchar"
					},
					{
						"name": "COMPLETION_STATUS",
						"type": "nvarchar"
					},
					{
						"name": "DATE_WELL_TEST",
						"type": "datetime",
						"precision": 23,
						"scale": 3
					},
					{
						"name": "OIL_VOL",
						"type": "float",
						"precision": 15
					},
					{
						"name": "GAS_VOL",
						"type": "float",
						"precision": 15
					},
					{
						"name": "WATER_VOL",
						"type": "float",
						"precision": 15
					}
				],
				"typeProperties": {
					"schema": "dbo",
					"table": "BE_WELL_TEST"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/SqlServerLocalJJJ')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/AzureBlobStorageCsvTwx')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobStorage",
				"typeProperties": {
					"connectionString": "[parameters('AzureBlobStorageCsvTwx_connectionString')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/SqlServerLocalJJJ')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "SqlServer",
				"typeProperties": {
					"connectionString": "[parameters('SqlServerLocalJJJ_connectionString')]"
				},
				"connectVia": {
					"referenceName": "integrationRuntimeLocalJJJ",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/integrationRuntimes/integrationRuntimeLocalJJJ')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/integrationRuntimeLocalJJJ')]",
			"type": "Microsoft.DataFactory/factories/integrationRuntimes",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "SelfHosted",
				"description": "Conexion Local JJJ",
				"typeProperties": {}
			},
			"dependsOn": []
		}
	]
}