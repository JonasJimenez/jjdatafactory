{
	"name": "GetWellAvocetShaya",
	"properties": {
		"activities": [
			{
				"name": "Copy Data Well",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlServerSource",
						"sqlReaderQuery": "SELECT START_DATETIME, END_DATETIME, WELL, WELL_ACTIVE, FIELD, PLATFORM, ZONA, TYPE, TYPE_TEXT, PRODUCT, PRODUCT_TEXT, STATUS, STATUS_TEXT, ZONE, ZONE_TEXT, PAD, PAD_TEXT, LIFT_TYPE, LIFT_TYPE_TEXT, SPHEROID, SPHEROID_TEXT, LATITUDE, LONGITUDE\nFROM BE_WELL",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureFileStorageWriteSettings",
							"copyBehavior": "FlattenHierarchy"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": {
								"value": "'Test_' + toString(currentDate()) + '.csv'",
								"type": "Expression"
							}
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "Input_DbAvocet",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "Ouput_ResourceSharedWell",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}